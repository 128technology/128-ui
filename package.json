{
  "name": "128-ui",
  "version": "0.0.2",
  "description": "128 Technology UI component library.",
  "main": "dist/128UI.js",
  "scripts": {
    "build:dev": "webpack --config webpack.development.config.js",
    "build:prod": "webpack --config webpack.production.config.js",
    "build": "npm run clean:dist && npm run build:dev && npm run build:prod",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "lint:js": "eslint --ext .js --ext .jsx src/components",
    "lint:style": "stylelint src/**/*.scss",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build",
    "clean:dist": "rimraf dist/",
    "clean:styleguide": "rimraf styleguide/",
    "clean": "npm run clean:dist && npm run clean:styleguide"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@bitbucket.128technology.com:7000/t128/node_modules.git"
  },
  "author": "128 Technology",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-import": "^1.1.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "loaders": "^1.1.3",
    "node-sass": "^4.5.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-styleguidist": "^5.0.4",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-merge": "^4.1.0"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "dependencies": {
    "antd": "^2.9.0"
  }
}
