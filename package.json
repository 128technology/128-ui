{
  "name": "@128technology/ui",
  "version": "0.2.1",
  "description": "128 Technology UI component library.",
  "main": "dist/128UI.js",
  "scripts": {
    "build:dev": "webpack --config webpack.development.config.js",
    "build:prod": "webpack --config webpack.production.config.js",
    "build": "npm run clean:dist && npm run build:dev && npm run build:prod",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "lint:js": "eslint --ext .js --ext .jsx src/components",
    "lint:style": "stylelint src/**/*.scss",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "mocha --require ./testSetup --compilers js:babel-core/register ./src/**/*-test.js* ./resetDom",
    "validate": "npm test && npm run lint",
    "prepublish": "npm run build",
    "clean:dist": "rimraf dist/",
    "clean:styleguide": "rimraf styleguide/",
    "clean": "npm run clean:dist && npm run clean:styleguide",
    "format": "prettier --print-width 120 --single-quote --trailing-comma none --write \"src/components/**/*.{js,jsx}\""
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/128technology/128-ui.git"
  },
  "author": "128 Technology",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-import": "^1.1.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.0-canary.1",
    "css-loader": "^0.28.0",
    "enzyme": "^3.3.0",
    "eslint": "^4.7.2",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^0.11.1",
    "ignore-styles": "^5.0.1",
    "jquery": "3.2.1",
    "jsdom": "^11.5.1",
    "mocha": "^3.3.0",
    "node-sass": "^4.5.2",
    "prettier": "^1.7.2",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-input-autosize": "^1.1.0",
    "react-styleguidist": "^7.0.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "sinon": "^2.2.0",
    "style-loader": "^0.16.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-merge": "^4.1.0"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "@128technology/react-select": "^1.2.1-A",
    "antd": "^3.4.0",
    "classnames": "^2.2.5",
    "enzyme-adapter-react-16": "^1.1.1",
    "material-ui": "^0.20.0",
    "mdi": "^1.9.33",
    "prop-types": "^15.5.8",
    "react-autocomplete": "^1.8.1",
    "react-tether": "^0.6.1",
    "react-virtualized": "^9.18.5",
    "react-virtualized-select": "^3.1.1"
  }
}
